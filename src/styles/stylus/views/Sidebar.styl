.sidebar .nav-link
  color: #333;
  font-size 14px
  &.info
      color $primary
      padding-top 0
      padding-bottom 0

.sidebar .nav-link .feather {
  margin-right: 4px;
  color: #999;
}

.sidebar .nav-link.active {
  color: $primary;
}

.sidebar .nav-link:hover .feather,
.sidebar .nav-link.active .feather {
  color: inherit;
}

.sidebar-heading {
  font-size: .75rem;
  text-transform: uppercase;
}

.navbar
    z-index 101
